{
    "name": "malvik-lab/laravel-http-logger",
    "description": "Log every request and response of Laravel PHP Framework.",
    "keywords": ["Laravel", "Log", "Request", "Response"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vittorio Malaspina",
            "email": "vittorio.malaspina@gmail.com"
        }
    ],
    "require": {},
    "autoload": {
        "psr-4": {
            "MalvikLab\\LaravelHttpLogger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MalvikLab\\LaravelHttpLogger\\Providers\\LaravelHttpLoggerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
